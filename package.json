{
  "name": "SocietyAPI",
  "version": "0.1.0-alpha.0",
  "description": "Society API",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t backend .",
    "docker:run": "docker run -p 3000:3000 -d backend",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "npm run clean && npm run build",
    "prestart": "npm run rebuild",
    "start": "nodemon dist/index.js",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/"
    ],
    "ignore": [
      "dist/*"
    ],
    "ext": "ts"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@emailjs/nodejs": "^2.1.1",
    "@loopback/authentication": "^7.0.2",
    "@loopback/authentication-jwt": "^0.7.2",
    "@loopback/authorization": "^0.7.2",
    "@loopback/boot": "^3.0.2",
    "@loopback/context": "^3.12.0",
    "@loopback/core": "^2.11.0",
    "@loopback/repository": "^3.1.0",
    "@loopback/rest": "^8.0.0",
    "@loopback/rest-explorer": "^3.0.2",
    "@loopback/security": "^0.3.2",
    "@loopback/service-proxy": "^3.0.2",
    "@types/jsonwebtoken": "8.5.0",
    "@types/multer": "^1.4.5",
    "aws-sdk": "^2.927.0",
    "bcryptjs": "^2.4.3",
    "casbin": "^5.1.2",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "loopback": "^3.28.0",
    "loopback-connector-mongodb": "^7.0.0-alpha.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.5",
    "@loopback/eslint-config": "^10.0.1",
    "@loopback/testlab": "^3.2.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^10.17.60",
    "eslint": "^7.10.0",
    "nodemon": "^2.0.4",
    "source-map-support": "^0.5.19",
    "typescript": "~4.0.6"
  }
}
